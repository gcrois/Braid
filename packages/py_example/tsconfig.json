{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"typeRoots": ["node_modules/@types", "declarations"],
		"declaration": true,
		"declarationDir": "dist/dts",
		"outDir": "dist/js",
		"sourceMap": true
	},
	"include": ["src", "declarations"]
}
